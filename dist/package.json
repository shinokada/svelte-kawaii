{
  "name": "svelte-kawaii",
  "version": "0.1.5",
  "description": "Svelte Kawaii is a Svelte port of React Kawaii, offering cute SVG illustrations to add adorable characters to your Svelte applications. A simple way to bring some cuteness to your UI.",
  "license": "MIT",
  "homepage": "https://svelte-kawaii.codewithshin.com/",
  "author": {
    "name": "Shinichi Okada",
    "email": "connect@codewithshin.com",
    "url": "https://codewithshin.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinokada/svelte-kawaii.git"
  },
  "bugs": "https://github.com/shinokada/svelte-kawaii/issues",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "keywords": [
    "svelte",
    "kawaii",
    "illustrations",
    "svg",
    "adorable",
    "components",
    "ui",
    "interface",
    "cartoon",
    "character",
    "mascot",
    "react-kawaii",
    "design"
  ],
  "contributors": [
    {
      "name": "react-kawaii/tree/main",
      "url": "https://github.com/elizabetdev/react-kawaii/tree/main"
    }
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "gen:exports": "svelte-lib-helpers exports",
    "copy:packagejson": "svelte-lib-helpers package",
    "copy:ts": "svelte-lib-helpers ts",
    "gen:docs5FromType": "svelte-lib-helpers docs5FromType",
    "lib-helpers": "pnpm format && pnpm gen:docs5FromType && pnpm  package && pnpm gen:exports && pnpm copy:packagejson",
    "ch": "npx changeset",
    "cv": "npx changeset version",
    "cp": "npx changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@playwright/test": "^1.51.1",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/package": "^2.3.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@tailwindcss/vite": "^4.0.17",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-svelte": "^3.3.3",
    "globals": "^16.0.0",
    "highlight.js": "^11.11.1",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "publint": "^0.3.9",
    "runatics": "^0.1.4",
    "runes-meta-tags": "^0.4.2",
    "runes-webkit": "^0.9.58",
    "svelte": "^5.25.3",
    "svelte-5-ui-lib": "^0.12.4",
    "svelte-check": "^4.1.5",
    "svelte-lib-helpers": "^0.4.18",
    "svelte-rune-highlight": "^0.5.15",
    "svelte-shields": "^0.5.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./Astronaut.svelte": {
      "types": "./dist/Astronaut.svelte.d.ts",
      "svelte": "./dist/Astronaut.svelte"
    },
    "./Backpack.svelte": {
      "types": "./dist/Backpack.svelte.d.ts",
      "svelte": "./dist/Backpack.svelte"
    },
    "./Browser.svelte": {
      "types": "./dist/Browser.svelte.d.ts",
      "svelte": "./dist/Browser.svelte"
    },
    "./Cat.svelte": {
      "types": "./dist/Cat.svelte.d.ts",
      "svelte": "./dist/Cat.svelte"
    },
    "./Chocolate.svelte": {
      "types": "./dist/Chocolate.svelte.d.ts",
      "svelte": "./dist/Chocolate.svelte"
    },
    "./CreditCard.svelte": {
      "types": "./dist/CreditCard.svelte.d.ts",
      "svelte": "./dist/CreditCard.svelte"
    },
    "./Cyborg.svelte": {
      "types": "./dist/Cyborg.svelte.d.ts",
      "svelte": "./dist/Cyborg.svelte"
    },
    "./Face.svelte": {
      "types": "./dist/Face.svelte.d.ts",
      "svelte": "./dist/Face.svelte"
    },
    "./File.svelte": {
      "types": "./dist/File.svelte.d.ts",
      "svelte": "./dist/File.svelte"
    },
    "./Folder.svelte": {
      "types": "./dist/Folder.svelte.d.ts",
      "svelte": "./dist/Folder.svelte"
    },
    "./Ghost.svelte": {
      "types": "./dist/Ghost.svelte.d.ts",
      "svelte": "./dist/Ghost.svelte"
    },
    "./HumanCat.svelte": {
      "types": "./dist/HumanCat.svelte.d.ts",
      "svelte": "./dist/HumanCat.svelte"
    },
    "./HumanDinosaur.svelte": {
      "types": "./dist/HumanDinosaur.svelte.d.ts",
      "svelte": "./dist/HumanDinosaur.svelte"
    },
    "./IceCream.svelte": {
      "types": "./dist/IceCream.svelte.d.ts",
      "svelte": "./dist/IceCream.svelte"
    },
    "./Mug.svelte": {
      "types": "./dist/Mug.svelte.d.ts",
      "svelte": "./dist/Mug.svelte"
    },
    "./Planet.svelte": {
      "types": "./dist/Planet.svelte.d.ts",
      "svelte": "./dist/Planet.svelte"
    },
    "./SpeechBubble.svelte": {
      "types": "./dist/SpeechBubble.svelte.d.ts",
      "svelte": "./dist/SpeechBubble.svelte"
    },
    "./package.json": "./package.json"
  }
}